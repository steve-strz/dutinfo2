package p1;

import java.util.*;
import java.time.LocalDateTime;

public class Message {
	private String title;
	private String content;
	private Membre autor;	
	private Date date;
	
	public Message(String t, String c, Membre m) {
		this.title = t;
		this.content = c;
		this.autor = m;
		this.date = new Date();
	}
	
	public Date getCreationDate() {
		return this.date;
	}
}
